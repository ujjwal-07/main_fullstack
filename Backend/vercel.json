{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/user/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/email/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/posts/(.*)",
        "dest": "/api/index.js"
      }
    ]
  }